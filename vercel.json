{
    "functions": {
        "api/**/*.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        { "src": "/(.*)", "dest": "/api/index.php" },
        { "src": "/(.*)", "dest": "/api/styles.css" },
        { "src": "/(.*)", "dest": "/api/database.css" },
        { "src": "/(.*)", "dest": "/api/authenticate.css" }
    ]
}